# http
export TF_ADDRESS=
export TF_USERNAME=service-account
export TF_PASSWORD=

# ПУТЬ до приватного ключа принадлежащего service account
export YC_KEY_SECRET=

# ПУТЬ до cloud-config для создаваемых VM-ок
# TF_VAR - обязательно, таким образом переменные из env оказываются в tf-коде при выполнении
# CLOUD_CONFIG - название перменной непосредственно в tf-коде
export TF_VAR_CLOUD_CONFIG=
