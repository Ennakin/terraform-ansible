# http
export TF_ADDRESS=
export TF_USERNAME=service
export TF_PASSWORD=

# путь до приватного ключа принадлежащего service account
export YC_KEY_SECRET_FILE_PATH=

# cloud-config для создаваемых VM-ок
# TF_VAR обязательно, таким образом переменные из env оказываются в tf-коде при выполнении
# cloud_config_file_path - название перменной непосредственно в tf-коде
export TF_VAR_cloud_config_file_path=
